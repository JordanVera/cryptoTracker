{
  "name": "coins",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@firebase/app": "^0.3.4",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.2",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "firebase": "^5.5.3",
    "lodash": "^4.17.10",
    "mongoose": "^5.3.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.3",
    "node-sass-chokidar": "0.0.3",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "path": "^0.12.7",
    "re-base": "^4.0.0",
    "react": "^16.4.1",
    "react-countup": "^4.0.0-alpha.6",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.1",
    "react-select": "^1.2.1",
    "react-transition-group": "^2.4.0",
    "react-virtual-list": "^2.3.0",
    "reactstrap": "^5.0.0",
    "recharts": "^1.0.1",
    "socket.io-client": "^2.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "dev": "concurrently \"react-scripts start\" \"nodemon server.js\"",
    "start": "npm-run-all -p watch-css build server-start",
    "server-start": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "extract-react-intl-messages": "^0.8.2"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  }
}
